.text_center.login_page_wrapper
  .page_inner
    h1.login_title 登録済ユーザーログインページ
    =form_for(:session, url: login_path) do |f| 
      =f.label :email 
      =f.email_field :email 
      br
      =f.label :password 
      =f.password_field :password 
      br
      =f.submit "Log in", class: "btn btn-primary" 
      br
      =link_to "新規ユーザー登録はこちら", new_user_path, class:"btn btn-danger"
